[%DOMAIN%]

user = www-%USER%
group = www-%USER%

listen = /run/php/php%PHP_VERSION%-%DOMAIN%.sock

listen.owner = www-data
listen.group = www-data
;listen.mode = 0660

pm = dynamic
pm.max_children = 4
pm.start_servers = 2
pm.min_spare_servers = 2
pm.max_spare_servers = 4
;pm.max_requests = 500
;pm.status_path = /status
;access.log = log/$pool.access.log
;chdir = /var/www
